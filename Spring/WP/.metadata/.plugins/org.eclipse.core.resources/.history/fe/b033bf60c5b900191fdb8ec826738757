package com.hsbc;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

@Component
public class A {
	B b;
	
	public A() {
		//super();
		System.out.println("A created using default constructor");
	}
	@Autowired
	public A(B b) {
		//super();
		System.out.println("A is created");
		this.b = b;
	}
	
}
