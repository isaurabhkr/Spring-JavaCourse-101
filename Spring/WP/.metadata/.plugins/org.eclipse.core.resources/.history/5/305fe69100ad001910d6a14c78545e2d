package collections;
import java.util.Stack;
public class TestStack {

	public static void main(String[] args) {
	Stack stck = new Stack<>();
	//generic way of creating a stack
	Stack<Integer> stckI = new Stack<Integer>();
	Stack<Integer> stckIn = new Stack<>();
	for(int i=0;i<20;i+=4)
	{
	stckI.push(i);
	}
	for(int i=0;i<stckI.size();i++)
	{
	System.out.println(stckI.elementAt(i));
	}
	
	
	}

}
