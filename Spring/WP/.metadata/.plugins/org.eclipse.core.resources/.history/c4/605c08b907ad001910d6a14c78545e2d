package collections;

import java.util.Vector;

public class TestVector {

	public static void main(String[] args) {
		Vector<Integer> intVec = new Vector<>(5,2);
		System.out.println("SIZE :: "+intVec.size());
		System.out.println("CAPACITY :: "+intVec.capacity());
		
		for(int i=0;i<9999921;i++) {
			intVec.addElement(i);
		}
		
		System.out.println("SIZE :: "+intVec.size());
		System.out.println("CAPACITY :: "+intVec.capacity());
	}

}
