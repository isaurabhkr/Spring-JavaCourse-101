package collectionsTest;
import collections.*;
import java.util.*;
public class TestStudentArray {

	public static void main(String[] args) {
		Student p1=new Student(1,"Narendra","Modi",65);
		Student p2=new Student(2,"Rahul","Gandhi",45);
		Student p3=new Student(3,"Sushil","Modi",65);
		Student p4=new Student(4,"Sonia","Gandhi",68);
		ArrayList<Student> studlist= new ArrayList<>();
		studlist.add(p1);
		studlist.add(p2);
		studlist.add(p3);
		studlist.add(p4);
		
		if( studlist.contains("p1")) 
			System.out.println("Student with id "+p1.getId()+" is found");
		else
			System.out.println("Student with id "+p1.getId()+" is not found");
	}

}
